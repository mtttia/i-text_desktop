{
  "name": "i-text",
  "version": "1.0.0",
  "description": "interactive-text",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=assets/icons/mac/icon.icns --prune=true --out=release-builds",
    "package-win": "electron-packager . i-text --overwrite --asar=false --platform=win32 --arch=ia32 --icon=src/icon/icon.ico --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"i-text\"",
    "package-linux": "electron-packager . i-text --overwrite --asar=true --platform=linux --arch=x64 --icon=src/icon/logo_bg-black.png --prune=true --out=release-builds"
  },
  "keywords": [],
  "author": "mtttia",
  "license": "ISC",
  "devDependencies": {
    "electron-packager": "^15.2.0"
  },
  "dependencies": {
    "@popperjs/core": "^2.6.0",
    "bootstrap": "^5.0.0-beta1",
    "jquery": "^3.5.1",
    "popper.js": "^1.16.1",
    "swiper": "^6.4.5"
  }
}
